{
  "name": "custom-coloring-book",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "dev:stripe": "stripe listen --forward-to localhost:3000/api/webhook/stripe",
    "dev:supabase": "npx supabase start",
    "dev:all": "concurrently -n supabase,next,inngest,stripe \"npm run dev:supabase\" \"npm run dev\" \"npm run dev:inngest\" \"npm run dev:stripe\"",
    "db:reset": "npx supabase db reset",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.29",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "ai": "^6.0.86",
    "inngest": "^3.52.0",
    "next": "16.1.6",
    "pdf-lib": "^1.17.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "replicate": "^1.4.0",
    "stripe": "^20.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "supabase": "^2.76.8",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
